{
  "compilerOptions": {
    "target": "es2016",
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "Components/*": ["./src/components/*"],
      "Helpers/*": ["./src/config/*"],
      "Store/*": ["./src/store/*"],
      "Constants/*": ["src/constants/*"],
      "Utils/*": ["src/utils/*"],
      "Api/*": ["src/api/*"],
      "Schemas/*": ["src/api/*"]
    },
    "module": "commonjs"
  },
  "exclude": ["node_modules", "dist"]
}
